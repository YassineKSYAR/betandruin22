                <TableColumn fx:id="qc1" prefWidth="39.0" text="QuestionN" />
                <TableColumn fx:id="qc2" prefWidth="352.0" text="Question" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Label layoutX="232.0" layoutY="49.0" text="Events" />
